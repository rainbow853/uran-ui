{
  "name": "@koala123/ui",
  "private": false,
  "author": "Uran Team",
  "version": "1.0.0",
  "description": "UI module for Uran",
  "type": "module",
  "module": "./lib/uran-ui.js",
  "main": "./lib/uran-ui.umd.cjs",
  "files": [
    "lib",
    "package/index.ts"
  ],
  "exports": {
    "./lib/style.css": "./lib/style.css",
    "./css": "./lib/style.css",
    ".": {
      "import": "./lib/uran-ui.js",
      "require": "./lib/uran-ui.umd.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.8.3",
    "kl-camera-frontend": "^1.0.3",
    "vue": "^3.4.37",
    "vue3-transform-dom": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "sass": "^1.77.8",
    "terser": "^5.31.6",
    "transform-image-data": "^2.0.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vitepress": "^1.5.0",
    "vue-tsc": "^2.0.29"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.d.ts",
    "src/**/*.d.ts"
  ]
}